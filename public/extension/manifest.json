{
    "manifest_version": 3,
    "name": "News Extension",
    "version": "1.0",
    "description": "A Chrome extension that shows local news",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": ["https://newsapi.org/"]
}
